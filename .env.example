# RTSP source
RTSP_URL=rtsp://user:password@192.168.1.50:554/stream1

# Dossier de sortie des enregistrements
OUTPUT_DIR=recordings

# Durée totale d'enregistrement (en jours)
RECORD_DAYS=3

# Durée d'un segment en secondes (600 = 10 minutes)
SEGMENT_SECONDS=600

# Format de sortie (mp4 ou mkv recommandé)
OUTPUT_FORMAT=mp4
